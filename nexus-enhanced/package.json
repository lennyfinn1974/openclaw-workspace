{
  "name": "nexus-enhanced",
  "version": "2.0.0",
  "description": "Comprehensive AI assistant integrating all OpenClaw capabilities",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run frontend\" \"npm run backend\"",
    "frontend": "cd frontend && npm run dev",
    "backend": "cd backend && python main.py",
    "build": "cd frontend && npm run build",
    "install:all": "npm install && cd frontend && npm install && cd ../backend && pip install -r requirements.txt",
    "start": "concurrently \"npm run backend\" \"npm run frontend:preview\"",
    "frontend:preview": "cd frontend && npm run preview",
    "test": "npm run test:frontend && npm run test:backend",
    "test:frontend": "cd frontend && npm test",
    "test:backend": "cd backend && python -m pytest",
    "deploy": "npm run build && npm run deploy:backend",
    "deploy:backend": "cd backend && python deploy.py"
  },
  "keywords": [
    "ai",
    "assistant", 
    "openclaw",
    "nexus",
    "skills",
    "plugins",
    "automation"
  ],
  "author": "Lenny Findlay",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^9.2.1"
  },
  "repository": {
    "type": "git",
    "url": "local"
  },
  "engines": {
    "node": ">=18.0.0",
    "python": ">=3.9.0"
  }
}